import Ember from 'ember';

export default Ember.Component.extend({
  computedProperty: Ember.computed('someKey', function() {
    return this.get('somethingAsync').then(somethingFetched => {
      return somethingFetched.getOtherStuff();
    }).then(() => {
      this.doAnotherThing();
    }).catch((error, status) => {
      this.doError(error, status);
    }).finally(this.cleanup);
  });
});
